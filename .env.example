# Configuration
CONFIG_PATH=./configs/example.config.yaml

# Convex Database
CONVEX_DEPLOY_URL=
CONVEX_AUTH_TOKEN=

# WorkOS Authentication
WORKOS_API_KEY=
WORKOS_CLIENT_ID=
WORKOS_REDIRECT_URI=http://localhost:3000/auth/callback

# Google Lab (Email & Embeddings)
GOOGLE_LAB_API_KEY=
GOOGLE_LAB_PROJECT_ID=

# Vapi (Voice & SMS)
VAPI_API_KEY=
VAPI_PUBLIC_KEY=
VAPI_PRIVATE_KEY=

# LiquidMetal (Search)
LIQUIDMETAL_API_KEY=

# Smithery (Deployment)
SMITHERY_TOKEN=
SMITHERY_PROJECT_ID=

# Application Settings
NODE_ENV=development
PORT=3000
ORCHESTRATOR_PORT=3001
ADMIN_UI_PORT=3002

# Logging
LOG_LEVEL=info
PII_MASKING_ENABLED=true

# Security
JWT_SECRET=your-jwt-secret-here
ENCRYPTION_KEY=your-encryption-key-here

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Timeouts (milliseconds)
HTTP_TIMEOUT=30000
DATABASE_TIMEOUT=10000